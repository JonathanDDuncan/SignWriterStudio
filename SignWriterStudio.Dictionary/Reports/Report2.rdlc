    End Function

    Shared Function LeftPaddingPixels(ByVal cellWidth As Single, ByVal cellHeight As Single, ByVal imageWidth As Single, ByVal imageHeight As Single) As Single
        Dim leftPadding = (cellWidth - imageWidth) / 2
        If leftPadding &lt; 0 Then leftPadding = 0
        Return leftPadding
    End Function

    Shared Function TopPaddingPixels(ByVal cellWidth As Single, ByVal cellHeight As Single, ByVal imageWidth As Single, ByVal imageHeight As Single) As Single
        Dim topPadding = (cellHeight - imageHeight) / 2
        If topPadding &lt; 0 Then topPadding = 0
        Return topPadding
    End Function

    Shared Function LeftPaddingProportionalCellInImagePxAsPtStr(ByVal cellWidthIn As Single, ByVal cellHeightIn As Single, ByVal imageWidthPx As Single, ByVal imageHeightPx As Single, ByVal dpi As Integer) As String
        Dim cellWidthPx = InToPx(cellWidthIn, dpi)
        Dim cellHeightPx = InToPx(cellHeightIn, dpi)
        Dim proportional = ProportionalSize(cellWidthPx, cellHeightPx, imageWidthPx, imageHeightPx)
        Return PttoStr(PxToPt(LeftPaddingPixels(cellWidthPx, cellHeightPx, proportional.Width, proportional.Height), dpi))
    End Function

    Shared Function TopPaddingProportionalCellInImagePxAsPtStr(ByVal cellWidthIn As Single, ByVal cellHeightIn As Single, ByVal imageWidthPx As Single, ByVal imageHeightPx As Single, ByVal dpi As Integer) As String
        Dim cellWidthPx = InToPx(cellWidthIn, dpi)
        Dim cellHeightPx = InToPx(cellHeightIn, dpi)
        Dim proportional = ProportionalSize(cellWidthPx, cellHeightPx, imageWidthPx, imageHeightPx)
        Return PttoStr(PxToPt(TopPaddingPixels(cellWidthPx, cellHeightPx, proportional.Width, proportional.Height), dpi))
    End Function

    Shared Function LeftPaddingCellInImagePxAsPtStr(ByVal cellWidthIn As Single, ByVal cellHeightIn As Single, ByVal imageWidthPx As Single, ByVal imageHeightPx As Single, ByVal dpi As Integer) As String
        Dim cellWidthPx = InToPx(cellWidthIn, dpi)
        Dim cellHeightPx = InToPx(cellHeightIn, dpi)

        Return PttoStr(PxToPt(LeftPaddingPixels(cellWidthPx, cellHeightPx, imageWidthPx, imageHeightPx), dpi))
    End Function
    Shared Function TopPaddingCellInImagePxAsPtStr(ByVal cellWidthIn As Single, ByVal cellHeightIn As Single, ByVal imageWidthPx As Single, ByVal imageHeightPx As Single, ByVal dpi As Integer) As String
        Dim cellWidthPx = InToPx(cellWidthIn, dpi)
        Dim cellHeightPx = InToPx(cellHeightIn, dpi)

        Return PttoStr(PxToPt(TopPaddingPixels(cellWidthPx, cellHeightPx, imageWidthPx, imageHeightPx), dpi))
    End Function

    Shared Function LeftPaddingProportionalCellPtImagePxAsPtStr(ByVal cellWidthPt As Single, ByVal cellHeightPt As Single, ByVal imageWidthPx As Single, ByVal imageHeightPx As Single, ByVal dpi As Integer) As String
        Dim cellWidthPx = PtToPx(cellWidthPt, dpi)
        Dim cellHeightPx = PtToPx(cellHeightPt, dpi)
        Dim proportional = ProportionalSize(cellWidthPx, cellHeightPx, imageWidthPx, imageHeightPx)
        Return PttoStr(PxToPt(LeftPaddingPixels(cellWidthPx, cellHeightPx, proportional.Width, proportional.Height), dpi))
    End Function

    Shared Function TopPaddingProportionalCellPtImagePxAsPtStr(ByVal cellWidthPt As Single, ByVal cellHeightPt As Single, ByVal imageWidthPx As Single, ByVal imageHeightPx As Single, ByVal dpi As Integer) As String
        Dim cellWidthPx = PtToPx(cellWidthPt, dpi)
