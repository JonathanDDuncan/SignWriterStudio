    Shared Function LeftPaddingPixelsProportional(ByVal cellWidth As Single, ByVal cellHeight As Single, ByVal imageWidth As Single, ByVal imageHeight As Single) As Single
        Dim proportional = ProportionalSize(cellWidth, cellHeight, imageWidth, imageHeight)
        Return LeftPaddingPixels(cellWidth, cellHeight, proportional.Width, proportional.Height)
    End Function
    Shared Function TopPaddingPixelsProportional(ByVal cellWidth As Single, ByVal cellHeight As Single, ByVal imageWidth As Single, ByVal imageHeight As Single) As Single
        Dim proportional = ProportionalSize(cellWidth, cellHeight, imageWidth, imageHeight)
        Return TopPaddingPixels(cellWidth, cellHeight, proportional.Width, proportional.Height)
    End Function

    Shared Function PxToPt(ByVal px As Single, Optional ByVal dpi As Integer = 96) As Single
        If (Not dpi = 0) Then
            Return px * 72 / dpi
        Else
            Return 0
        End If
    End Function
    Shared Function PtToPx(ByVal pt As Single, Optional ByVal dpi As Integer = 96) As Single
        Return (pt * dpi) / 72
    End Function
    Shared Function PxToIn(ByVal px As Single, Optional ByVal dpi As Integer = 96) As Single
        If (Not dpi = 0) Then
            Return px / dpi
        Else
            Return 0
        End If
    End Function
    Shared Function InToPx(ByVal inches As Single, Optional ByVal dpi As Integer = 96) As Single
        Return inches * dpi
    End Function
    Shared Function ProportionalSize(cellwidth As Single, cellheight As Single, imgwidth As Single, imgheight As Single) As System.Drawing.SizeF
        Dim ratio As Single = 1

        If imgwidth &gt; imgheight AndAlso Not Math.Abs(imgwidth - 0) &lt; Single.Epsilon Then
            ratio = cellwidth / imgwidth
        ElseIf Not Math.Abs(imgheight - 0) &lt; Single.Epsilon Then
            ratio = cellheight / imgheight
        End If

        Return New System.Drawing.SizeF(imgwidth * ratio, imgheight * ratio)
    End Function

    Shared Function PttoStr(point As Single) As String
        Return point.ToString() + "pt"
    End Function
