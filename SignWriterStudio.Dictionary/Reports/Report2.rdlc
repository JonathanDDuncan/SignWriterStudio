    Shared Function GetTopPadding(ByVal cellWidthIn As Single, ByVal cellHeightIn As Single, ByVal imageWidthPx As Single, ByVal imageHeightPx As Single, ByVal dpi As Integer) As String
        Return TopPaddingCellInImagePxAsPtStr(cellWidthIn, cellHeightIn, imageWidthPx, imageHeightPx, dpi)
    End Function
    Shared Function GetLeftPadding(ByVal cellWidthIn As Single, ByVal cellHeightIn As Single, ByVal imageWidthPx As Single, ByVal imageHeightPx As Single, ByVal dpi As Integer) As String
