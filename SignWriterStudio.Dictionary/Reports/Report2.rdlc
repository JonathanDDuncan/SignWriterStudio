        Dim proportional = ProportionalSize(cellWidthPx, cellHeightPx, imageWidthPx, imageHeightPx)
        Return PttoStr(PxToPt(TopPaddingPixels(cellWidthPx, cellHeightPx, proportional.Width, proportional.Height), dpi))
    End Function

